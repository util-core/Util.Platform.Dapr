@model ResourceQuery

@*查询表单*@
<util-form class="search__form">
    <util-row gutter="{xs: 8, sm: 16, md: 24, lg: 32, xl: 32, xxl: 32}">
        <util-column Xs="24" Sm="12" Md="10" Lg="6" Xl="4">
            <util-input for="Name" label-text="identity.module.name" />
        </util-column>
        <util-column Xs="24" Sm="12" Md="10" Lg="6" Xl="4">
            <util-input for="Uri" label-text="identity.module.uri" />
        </util-column>
        <util-column Xs="24" Sm="12" Md="10" Lg="6" Xl="4">
            <util-select for="Enabled"></util-select>
        </util-column>
        <util-column class="mb-sm">
            <util-button id="btnQuery" type="Primary" icon="Search" on-click="query(btnQuery)"  text-query="true"></util-button>
            <util-button class="mx-sm" icon="Sync" on-click="refresh()" text-refresh="true"></util-button>
        </util-column>
    </util-row>
</util-form>