@page
@model ModuleDto
@*标题*@
<util-page-header2 title-create="identity.module.create" title-update="identity.module.update" auto-breadcrumb="false" sync-title="false"></util-page-header2>
@*表单*@
<util-card borderless="true">
    <util-form label-span="3" control-span="21" gutter="16">
        <util-display for="ApplicationName" show-label="true" value="applicationName"></util-display>
        <util-tree-select for="ParentId" query-param="queryParam" load-mode="Sync" url="module/tree" default-expand-all="true"
                          sort="SortId" label-text="identity.module.parentId" control-span="8"></util-tree-select>
        <util-input for="Name"/>
        <util-input for="Uri"/>
        <util-input for="Icon"/>
        <util-switch for="Expanded" />
        <util-switch for="Enabled"/>
        <util-input-number for="SortId"/>
        <util-textarea for="Remark"></util-textarea>
        <util-row justify="End">
            <util-button class="mr-sm" on-click="close()" text-cancel="true"></util-button>
            <util-button id="btnSubmit" is-submit="true" validate-form="true" type="Primary" on-click="submit(btnSubmit)" text-ok="true"></util-button>
        </util-row>
    </util-form>
</util-card>