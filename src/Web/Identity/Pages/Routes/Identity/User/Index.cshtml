@page
@model UserDto
<util-page-header2 title="identity.user"></util-page-header2>
<util-card borderless="true">
    @*查询表单*@
    <partial name="Index.Query" />
    @*工具按钮*@
    <util-button type="Primary" icon="Plus" on-click="openCreateDialog()" text-create="true"></util-button>
    <util-button type="Primary" Danger="true" icon="Delete" on-click="delete()" text-delete="true"></util-button>
    @*提示语*@
    <util-alert type="Info" show-icon="true" class="my-md">
        <util-i18n key="app.rowsSelected" param="length:getCheckedLength()" />
    </util-alert>
    @*表格*@
    <util-table id="tb_user" show-checkbox="true" show-line-number="true" query-param="queryParam" url="user" sort="CreationTime desc">
        <util-td for="UserName"></util-td>
        <util-td for="Email"></util-td>
        <util-td for="PhoneNumber"></util-td>
        <util-td for="Enabled"></util-td>
        <util-td for="LoginCount"></util-td>
        <util-td for="RegisterIp"></util-td>
        <util-td for="LastLoginTime"></util-td>
        <util-td for="LastLoginIp"></util-td>
        <util-td for="CurrentLoginTime"></util-td>
        <util-td for="CurrentLoginIp"></util-td>
        <util-td for="CreationTime"></util-td>
        <util-td title-operation="true">
            <util-a on-click="openDetailDialog(row)" text-detail="true"></util-a>
            <util-divider type="Vertical"></util-divider>
            <util-a danger="true" on-click="delete(row.id)" text-delete="true"></util-a>
        </util-td>
    </util-table>
</util-card>