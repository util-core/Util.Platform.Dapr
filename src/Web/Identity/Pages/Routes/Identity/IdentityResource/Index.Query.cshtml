@model ResourceQuery

@*查询表单*@
<util-form class="search__form mb-sm">
    <util-row gutter="24">
        <util-column xxl="4" md="8" xs="24">
            <util-input for="Uri" label-text="identity.identityResource.uri" />
        </util-column>
        <util-column xxl="4" md="8" xs="24">
            <util-input for="Name" label-text="identity.identityResource.name" />
        </util-column>
        <util-column>
            <util-button id="btnQuery" type="Primary" icon="Search" on-click="query(btnQuery)" text-query="true"></util-button>
            <util-button id="btnRefresh" icon="Sync" on-click="refresh(btnRefresh)" text-refresh="true"></util-button>
        </util-column>
    </util-row>
</util-form>